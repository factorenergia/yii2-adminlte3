{
    "name": "factorenergia/yii2-adminlte3",
    "description": "yii2-adminlte3 is a package for Tii framework 2 that consists a very popular admin template AdminLTE (Based on bootstrap4)",
    "keywords": [
        "yii2",
        "admin",
        "template",
        "theme",
        "backend",
        "AdminLTE",
        "bootstrap4"
    ],
    "homepage": "https://github.com/factorenergia/yii2-adminlte3",
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/factorenergia/yii2-adminlte3/issues",
        "source": "https://github.com/factorenergia/yii2-adminlte3"
    },
    "authors": [
        {
            "name": "Josep Vidal",
            "email": "jvidal@factorenergia.com",
            "homepage": "https://github.com/jvidalv",
            "role": "Creator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": ">=2.0.14",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "almasaeed2010/adminlte": "~3.0",
        "rmrevin/yii2-fontawesome": "~3.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "factorenergia\\adminlte\\": ""
        }
    }
}
